@{
}
<style>
    table {
        border-collapse: collapse;
        width: 20%;
    }

    th, td {
        border: 1px solid #ddd;
        padding: 8px;
        text-align: left;
    }

    th {
        background-color: #f2f2f2;
    }
</style>
<table >

    <tr>

        <th>Key</th>
        <th>Value</th>
    </tr>
    @foreach (var dict in Model)
    {
        <tr>
            <td>@dict.Key</td>
            <td>@dict.Value</td>
        </tr>
    }
    
</table>
